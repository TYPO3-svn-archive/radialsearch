<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
  <ROOT>
    <TCEforms>
      <sheetTitle>LLL:EXT:radialsearch/pi2/locallang_flexform.xml:sdef</sheetTitle>
    </TCEforms>
    <type>array</type>
    <el>
      <sdefReportEnable>
        <TCEforms>
          <label>LLL:EXT:radialsearch/pi2/locallang_flexform.xml:sdefReportEnable</label>
          <onChange>reload</onChange>
          <config>
            <type>check</type>
            <default>0</default>
          </config>
        </TCEforms>
      </sdefReportEnable>
      <sdefReport>
          <label>LLL:EXT:radialsearch/pi2/locallang_flexform.xml:sdefReport</label>
        <TCEforms>
          <config>
            <type>user</type>
            <userFunc>tx_radialsearch_userfunc->pi2FfSdefReport</userFunc>
          </config>
        </TCEforms>
      </sdefReport>
      <sdefDrs>
        <TCEforms>
          <label>LLL:EXT:radialsearch/pi2/locallang_flexform.xml:sdefDrs</label>
          <config>
            <type>check</type>
            <default>0</default>
          </config>
        </TCEforms>
      </sdefDrs>
      <sdefUpdatewizard>
        <TCEforms>
          <label>LLL:EXT:radialsearch/pi2/locallang_flexform.xml:sdefUpdatewizard</label>
          <config>
            <type>check</type>
            <default>0</default>
          </config>
        </TCEforms>
      </sdefUpdatewizard>
      <sdefCurrip>
        <TCEforms>
          <config>
            <type>user</type>
            <userFunc>tx_radialsearch_userfunc->promptCurrIP</userFunc>
          </config>
        </TCEforms>
      </sdefCurrip>
      <sdefCsvallowedip>
        <TCEforms>
          <label>LLL:EXT:radialsearch/pi2/locallang_flexform.xml:sdefCsvallowedip</label>
          <config>
            <type>input</type>
            <size>40</size>
            <max>255</max>
            <eval>trim</eval>
            <default>127.0.0.1, 127.0.0.2</default>
          </config>
        </TCEforms>
      </sdefCsvallowedip>
    </el>
  </ROOT>
</T3DataStructure>
